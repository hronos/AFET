    <ul>
        <li><input type="checkbox" name="distance" value="29"/>Distance number</li>
        <li><input type="checkbox" name="math" value="20"/>Basic maths</li>
        <li><input type="checkbox" name="decimals" value="24"/>Decimals</li>
        <li><input type="checkbox" name="fractions" value="25"/>Fractions</li>
