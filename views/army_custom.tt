    <ul>
        <li><input type="checkbox" name="rotation" value="1"/>Symbol rotation</li>
        <li><input type="checkbox" name="pattern" value="4"/>Patterns</li>
    </ul>
    <li>Verbal reasoning</li>
    <ul>
        <li><input type="checkbox" name="relationships" value="5"/>Relationships</li>
        <li><input type="checkbox" name="letter" value="6"/>Letter checking</li>
